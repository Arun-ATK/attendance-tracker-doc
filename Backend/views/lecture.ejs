<html>
<body>
<table class="lecture-table" border=1 cell-spacing=0>
  <thead>
    <tr>
      <th>Lecture id</th>
      <th>Class id</th>
      <th>Duration</th>
      <th>Start time</th>
      <th>Threshold Percent</th>
    </tr>
  </thead>

  <tbody>
    <% for(var i=0; i < faculty.length; i++) { %>
    <tr>
     <td><%= faculty[i].lecture_id %></td>
	<td><%= faculty[i].class_id %></td>
     <td><%= faculty[i].duration %></td>
      <td><%= faculty[i].start_time %></td>
      <td><%= faculty[i].threshold_percent %></td>
    </tr>
    <% } %>
  </tbody>
  </table>
  <style>
  body{
       font-size : 25px;
       background:linear-gradient(#07F5F5,white);
       position:absolute;
       top:50%;
       left:50%;
       transform: translate(-50%,-50%);
       text-align:center;
  }
 </style>
</body>
</html>
